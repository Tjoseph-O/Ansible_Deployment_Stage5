{
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "{{ cloudwatch_logs.app.files[0] }}",
            "log_group_name": "{{ cloudwatch_logs.app.log_group }}",
            "log_stream_name": "{{ env_name }}-app",
            "timestamp_format": "%Y-%m-%d %H:%M:%S"
          },
          {
            "file_path": "{{ cloudwatch_logs.nginx.files[0] }}",
            "log_group_name": "{{ cloudwatch_logs.nginx.log_group }}",
            "log_stream_name": "{{ env_name }}-nginx-access",
            "timestamp_format": "%d/%b/%Y:%H:%M:%S %z"
          },
          {
            "file_path": "{{ cloudwatch_logs.nginx.files[1] }}",
            "log_group_name": "{{ cloudwatch_logs.nginx.log_group }}",
            "log_stream_name": "{{ env_name }}-nginx-error",
            "timestamp_format": "%Y/%m/%d %H:%M:%S"
          }
        ]
      }
    },
    "force_flush_interval": 15
  }
}
